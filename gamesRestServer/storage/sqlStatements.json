{
  "getAll": ["select number,name,quantity,year,genre from game"],
  "get": ["select number,name,quantity,year,genre from game", "where number=?"],
  "insert": [
    "insert into game (number,name,quantity,year,genre)",
    "values(?,?,?,?,?)"
  ],
  "update": [
    "update game set name=?, quantity=?, year=?, genre=?",
    "where number=?"
  ],
  "remove": ["delete from game where number=?"],
  "primaryKey": "number",
  "resource": "games"
}
